<form method="post" action="{{ path('apprenant_delete', {'id': apprenant.id}) }}"
 onsubmit="return confirm('Etes-vous sure de vouloir supprimer cet apprenant?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ apprenant.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
